{
  "compilerOptions": {
    "target": "es2017",
    "module": "es2015",
    "removeComments": true,
    "lib": ["es2017", "esnext.asynciterable", "dom"],
    "types": [],
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "inlineSources": true,
    "outDir": "./dist/esm/",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitThis": true
  },
  "include": [
    "src/**/*.ts",
    "byted-tea-sdk.d.ts",
    "event_target_shim.d.ts"
  ],
  "exclude": []
}
